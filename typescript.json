{
	"function": {
        "prefix": "const",
        "body": [
            "${1:const} ${2:funcname} = function($3) : ${4:void} {",
            "    $5",
            "}",
        ],
        "description": ""
    },
	"async function": {
        "prefix": "const asc",
        "body": [
            "${1:const} ${2:funcname} = async function($3) : Promise<${4:void}> {",
            "    $5",
            "}",
        ],
        "description": ""
    },
	"arrow async function": {
        "prefix": "const arw asc",
        "body": [
            "${1:const} ${2:funcname} = async ($3) : Promise<${4:void}> => {",
            "    $5",
            "}",
        ],
        "description": ""
    },
	"arrow function": {
        "prefix": "const arw",
        "body": [
            "${1:const} ${2:funcname} = ($3) : ${4:void} => {",
            "    $5",
            "}",
        ],
        "description": ""
    },
    "Print to console": {
		"prefix": "log",
		"body": [
			"console.log('$1')"
		],
		"description": "Log output to console"
	},
	"if": {
        "prefix": "if",
        "body": [
            "if ($1) {",
            "    ",
            "}",
        ],
        "description": ""
    },

    "arrow": {
        "prefix": "arw",
        "body": [
            "=>"
        ],
        "description": ""
    },
    "for": {
        "prefix": "for",
        "body": [
            "for (let i = 0 ; i < ${1:ary}.length; i++){",
            "    let elm = ${1:ary}[i]",
            "}",
        ],
        "description": ""
    },
	// Place your snippets for typescript here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
}